	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/utilities.o utilities.cpp

obj/networkclient.o: networkclient.cpp networkclient.h \
		peerstate.h \
		mapmarker.h \
		wedge.h \
		global.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/networkclient.o networkclient.cpp

obj/peerstate.o: peerstate.cpp peerstate.h \
		mapmarker.h \
		wedge.h \
		global.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/peerstate.o peerstate.cpp

obj/moc_geomap.o: moc/moc_geomap.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/moc_geomap.o moc/moc_geomap.cpp

obj/moc_mappingwidget.o: moc/moc_mappingwidget.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/moc_mappingwidget.o moc/moc_mappingwidget.cpp

obj/moc_networkclient.o: moc/moc_networkclient.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/moc_networkclient.o moc/moc_networkclient.cpp

obj/qrc_Icons.o: rcc/qrc_Icons.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/qrc_Icons.o rcc/qrc_Icons.cpp

####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/usr/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/usr/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/usr/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/usr/bin/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

